services:
  genericapp.api:
    build:
      context: C:\Users\mmcdermott\source\repos\POC\DistributedTracing\GenericApp
      dockerfile: GenericApp.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: GenericApp.Api
      target: base
    container_name: GenericApp.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: genericappapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/GenericApp.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 55080
      target: 80
    tty: true
    volumes:
    - C:\Users\mmcdermott\source\repos\POC\DistributedTracing\GenericApp\GenericApp.Api:/app:rw
    - C:\Users\mmcdermott\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\mmcdermott\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\mmcdermott\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\mmcdermott\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\mmcdermott\source\repos\POC\DistributedTracing\GenericApp:/src:rw
  genericapp.web:
    build:
      context: C:\Users\mmcdermott\source\repos\POC\DistributedTracing\GenericApp
      dockerfile: GenericApp.Web/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: GenericApp.Web
      target: base
    container_name: GenericApp.Web
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: genericappweb:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/GenericApp.Web.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 56080
      target: 80
    tty: true
    volumes:
    - C:\Users\mmcdermott\source\repos\POC\DistributedTracing\GenericApp\GenericApp.Web:/app:rw
    - C:\Users\mmcdermott\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\mmcdermott\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\mmcdermott\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\mmcdermott\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\mmcdermott\source\repos\POC\DistributedTracing\GenericApp:/src:rw
